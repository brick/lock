{
    "name": "brick/lock",
    "description": "Advisory locks library",
    "type": "library",
    "keywords": [
        "Brick",
        "Lock",
        "Mutex"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "ext-pdo": "*",
        "doctrine/dbal": "3.* || 4.*",
        "phpstan/phpstan": "2.1.22",
        "phpunit/phpunit": "^11.5",
        "symfony/process": "^7.3",
        "opis/closure": "^4.3",
        "php-coveralls/php-coveralls": "^2.8",
        "guzzlehttp/guzzle": "^7.0"
    },
    "conflict": {
        "doctrine/dbal": "<3.0 || >=5.0"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Lock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\Lock\\Tests\\": "tests/"
        }
    }
}
